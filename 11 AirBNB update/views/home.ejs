<%- include('partials/head') %>
  </head>
  <body>
    <%- include('partials/nav') %>
    <main class="container mx-auto bg-white shadow-lg rounded-lg p-8 mt-10 max-w-xl">
      <h2 class="text-3xl text-red-500 font-bold text-center mb-6">
        Here are our registered homes:
      </h2>
      <ul class="space-y-4">
        <% registeredHomes.forEach(home => { %>
          <li class="bg-gray-100 p-4 rounded-lg shadow-md hover:shadow-lg transition duration-300">
            <div class="text-lg text-gray-800">House Name: <%= home.houseName %></div>
            <div class="text-lg text-gray-800">House Location: <%= home.houseLocation%></div>
            <div class="text-lg text-gray-800">House Price: <%= home.housePrice  %></div>
            <div class="text-lg text-gray-800">House Rating: <%= home.houseRating %></div>
            <a href= <%= home.housePhotos %>  class="text-lg text-blue-500">click for House Photos</a>
          </li>
        <% }) %>
      </ul>
    </main>
  </body>
</html>